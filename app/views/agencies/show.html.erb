
<p id="notice"><%= notice %></p>

<table summary="Agency Info" width="100%" class="table table-bordered">
<thead>
<tr>
<th style="text-align:center"><h4><%= @agency.name %></h4></th>

</tr>
<tr>
	<th>Security Code</th>
</tr>
</thead>
<tbody>
<tr>
<td>
 <h6><%= @agency.security_code %></h6>
</td>
</tr>
</tbody>
<br class="clearfix">
<tfoot>
<tr>
	<td>
<div class="pull-right">
	<%= link_to 'Edit', edit_agency_path(@agency),:class=>"btn btn-primary" %>
	<%= link_to 'Back', agencies_path, :class=>"btn btn-primary" %>
	
</div>
</td>
</tr>
</tfoot>
</table>
<br class="clearfix">
<br class="clearfix">


<table width="100%"class="table table-bordered" id="agency-users">
	<tfoot>
<tr>
	<td>
		<div class="pull-right">
<%= link_to "Add User", new_agency_user_url(@agency), :class=>"btn btn-primary" %>
</div>
</td>
</tr>
</tfoot>

<thead>
<tr>
	<th style="text-align:center;"><h4>List of all users</h4></th>
	
</tr>

</thead>

<tbody>
	<% @agency.users.each do |user| %>
<tr>
	<td>
		
		<h6><%= user.email %></h6>
		
	</td>
</tr>
<% end %>
</tbody>
</table>

<br class="clearfix">

<!-- ALL PPPM -->

<table width="100%" id="all-ppmps">
	<thead>
	<tr>
		<th style="text-align:center;">
		<h4>List of all PPMP's</h4>
		</th>
	</tr>
	
	<tr>
		<th style="text-align:left;">Name</th>
	</tr>
	</thead>

	<tbody>
		<% @agency.project_procurement_management_plans.each do |ppmp| %>
	<tr>
		<td>
		
			<h6><%= ppmp.name %></h6>
		
		</td>
	</tr>
	<% end %>
	</tbody>
</table>



	